{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.3.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --prod",
    "build:docker": "docker build . -t textbook/salary-stats",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "pree2e": "webdriver-manager update --gecko false",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json",
    "lint": "ng lint",
    "ng": "ng",
    "ng:update": "npm run ng -- update @angular/{cli,core}",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e && npm run build",
    "start": "ng serve",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "11.0.2",
    "@angular/compiler": "11.0.2",
    "@angular/core": "11.0.2",
    "@angular/forms": "11.0.2",
    "@angular/platform-browser": "11.0.2",
    "@angular/platform-browser-dynamic": "11.0.2",
    "@angular/router": "11.0.2",
    "angular-in-memory-web-api": "0.11.0",
    "highcharts": "^8.2.2",
    "highcharts-angular": "^2.8.2",
    "jerzy": "^0.2.1",
    "request": "^2.88.2",
    "rxjs": "^6.6.3",
    "simple-statistics": "^7.3.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-eslint/builder": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "0.6.0-beta.0",
    "@angular-eslint/schematics": "^0.6.0-beta.0",
    "@angular-eslint/template-parser": "0.6.0-beta.0",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "11.0.2",
    "@angular/language-service": "11.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "4.0.3"
  }
}
