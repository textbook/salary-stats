{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.6.5",
  "license": "ISC",
  "scripts": {
    "build": "ng build --configuration production",
    "build:docker": "docker build . --build-arg \"NODE_RELEASE=$(cat .nvmrc)\" --build-arg ALPINE_RELEASE=3.17 -t textbook/salary-stats",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json",
    "lint": "ng lint",
    "ng": "ng",
    "ng:update": "npm run ng -- update @angular/{cli,core}",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e && npm run build",
    "start": "ng serve",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "^15.2.6",
    "@angular/compiler": "^15.2.6",
    "@angular/core": "^15.2.6",
    "@angular/forms": "^15.2.6",
    "@angular/platform-browser": "^15.2.6",
    "@angular/platform-browser-dynamic": "^15.2.6",
    "@angular/router": "^15.2.6",
    "angular-in-memory-web-api": "^0.15.0",
    "highcharts": "^10.3.3",
    "highcharts-angular": "^3.1.2",
    "jerzy": "^0.2.1",
    "rxjs": "~7.5.7",
    "simple-statistics": "^7.8.3",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1502.5",
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.5",
    "@angular/compiler-cli": "^15.2.6",
    "@cypress/schematic": "2.5.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/jasmine": "~4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cypress": "12.9.0",
    "eslint": "^8.38.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4"
  }
}
