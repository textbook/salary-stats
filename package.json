{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.2.0",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --prod",
    "build:docker": "docker build . -t textbook/salary-stats",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "pree2e": "webdriver-manager update --gecko false",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json",
    "lint": "ng lint",
    "ng": "ng",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e && npm run build",
    "start": "ng serve",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "10.0.14",
    "@angular/compiler": "10.0.14",
    "@angular/core": "10.0.14",
    "@angular/forms": "10.0.14",
    "@angular/platform-browser": "10.0.14",
    "@angular/platform-browser-dynamic": "10.0.14",
    "@angular/router": "10.0.14",
    "angular-in-memory-web-api": "0.11.0",
    "highcharts": "^8.1.2",
    "highcharts-angular": "^2.4.0",
    "jerzy": "^0.2.1",
    "request": "^2.88.2",
    "rxjs": "^6.6.0",
    "simple-statistics": "^7.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "10.0.14",
    "@angular/language-service": "10.0.14",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "coveralls": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.5"
  }
}
