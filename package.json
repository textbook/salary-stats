{
  "name": "salary-stats",
  "description": "Analyse and compare salaries by cohort.",
  "keywords": [
    "salary",
    "statistics",
    "comparison",
    "angular",
    "highcharts",
    "bulma"
  ],
  "version": "0.3.5",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --configuration production",
    "build:docker": "docker build . --build-arg \"NODE_RELEASE=$(cat .nvmrc)\" --build-arg ALPINE_RELEASE=3.13 -t textbook/salary-stats",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "pree2e": "webdriver-manager update --gecko false --versions.chrome=\"$(google-chrome --product-version || echo 'latest')\"",
    "e2e": "ng e2e --no-webdriver-update",
    "postinstall": "ngcc --tsconfig ./src/tsconfig.app.json",
    "lint": "ng lint",
    "ng": "ng",
    "ng:update": "npm run ng -- update @angular/{cli,core}",
    "reinstall": "rm -rf ./node_modules && npm install",
    "ship": "npm run lint && npm run test && npm run e2e && npm run build",
    "start": "ng serve",
    "test": "ng test --no-watch",
    "test:cover": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "test:watch": "ng test"
  },
  "private": true,
  "author": "Jonathan Sharpe",
  "contributors": [
    "Beat Richartz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/textbook/salary-stats"
  },
  "homepage": "https://github.com/textbook/salary-stats",
  "bugs": "https://github.com/textbook/salary-stats/issues",
  "dependencies": {
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "angular-in-memory-web-api": "0.11.0",
    "highcharts": "^9.1.0",
    "highcharts-angular": "^2.10.0",
    "jerzy": "^0.2.1",
    "request": "^2.88.2",
    "rxjs": "^6.6.7",
    "simple-statistics": "^7.7.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular-eslint/eslint-plugin-template": "^12.3.1",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "^12.3.1",
    "@angular/cli": "^12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@angular/language-service": "12.0.0",
    "@types/jasmine": "~3.6.11",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^12.20.13",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "coveralls": "^3.1.0",
    "eslint": "^7.31.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-mocha-reporter": "^2.2.5",
    "protractor": "~7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "4.2.4"
  }
}
